{
  "name": "magento2-vue-checkout",
  "version": "1.0.0",
  "description": "Magento 2 Checkout module based on Vue.js",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "APP_ENV=development vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --fix .",
    "format": "prettier .  --write"
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "\\.(svg)$": "<rootDir>/node_modules/vue-svg-loader"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "dependencies": {
    "@formkit/themes": "^1.0.0-beta.4",
    "@formkit/vue": "^1.0.0-beta.4",
    "axios": "^0.26.0",
    "vue": "^3.2.31",
    "vue-axios": "^3.4.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/compiler-sfc": "^3.2.31",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.5.0",
    "prettier": "2.5.1",
    "vite": "^2.8.4",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-live-reload": "^2.1.0",
    "vite-plugin-restart": "^0.0.2"
  }
}
